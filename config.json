{
  "name": "Access Control",
  "version": "dev",
  "slug": "access_control",
  "description": "Access control add-on",
  "url": "https://github.com/hassio-addons/addon-example",
  "webui": "http://[HOST]:[PORT:8100]",
  "startup": "services",
  "panel_icon": "mdi:code-brackets",
  "panel_title": "Access Control",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "auth_api": true,
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "ingress": true,
  "ingress_port": 1337,
  "options": {
    "log_level": "info",
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "mqtt": 
    {
      "host": "localhost",
      "username": "user",
      "password": "pass",
      "client_id": "client",
      "port": 1883
    },
    "grpc":
    {
      "port" : 5055
    }
  
  },
  "schema": {
    "log_level": "list(trace|debug|info|notice|warning|error|fatal)",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "i_like_to_be_pwned": "bool?",
    "leave_front_door_open": "bool?",
    "mqtt":
    {
      "host": "str",
      "username": "str",
      "password": "str",
      "client_id": "str",
      "port": "port"
    },
    "grpc":
    {
      "port" : "port"
    }
  },
  "ports": {
    "80/tcp": null
  },
  "ports_description": {
    "80/tcp": "Access Control web interface"
  }
}
